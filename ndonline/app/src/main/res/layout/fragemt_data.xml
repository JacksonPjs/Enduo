<?xml version="1.0" encoding="utf-8"?>
<com.enduo.ndonline.ui.widget.MyScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/myscrollview"
    android:fillViewport="true"
    android:scrollbars="none"

    >
    <com.pvj.xlibrary.loadinglayout.LoadingLayout
        android:id="@+id/layout_contiant_girdview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <GridView
            android:id="@+id/girdview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/bg_huise"
            android:horizontalSpacing="5dp"
            android:numColumns="2">
        </GridView>
    </com.pvj.xlibrary.loadinglayout.LoadingLayout>

</com.enduo.ndonline.ui.widget.MyScrollView>